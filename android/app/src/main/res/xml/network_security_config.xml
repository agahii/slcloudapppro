<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext HTTP only for your API host (or IP) -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">api.slcloud.3em.tech</domain>
        <!-- For an IP instead: <domain>192.168.1.50</domain> -->
    </domain-config>
</network-security-config>
